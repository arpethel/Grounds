@model IEnumerable<WebApplication1.Models.Resource>;

@{
    ViewData["Title"] = "Home Page";
}


<div class="row card" style="overflow:scroll; height: 700px">
    <!--display resources here-->
    <ul class="list-group list-group-flush">
        @foreach (var item in Model)
        {
            <li class="list-group-item">
                <h3>@Html.DisplayFor(modelItem => item.ResourceTitle)</h3>
                <p>@Html.DisplayFor(modelItem => item.ResourceDescription)</p>
            </li>
        }
    </ul>
</div>